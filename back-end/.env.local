# Cashbox starting code (4-digit).
# When unset, server defaults to 6000.
CASHBOX_START_CODE=6000

# Bank account Details starting code (4-digit).
# When unset, server defaults to 6100.
BANK_DETAIL_START_CODE=6100

# Accounting runs as a schema inside the Greenbunch database
ACCOUNTING_SCHEMA=accounting

# Local Postgres connection for development (points to greenbunch DB)
DATABASE_URL=postgres://postgres:1qaz@localhost:5432/greenbunch?sslmode=disable
PGSSLMODE=disable
DISABLE_REFRESH_PERSISTENCE=true

# Align JWT secret with main server so admin tokens work in accounting
JWT_SECRET=your-super-secret-jwt-key-for-development